Data Dictionary =
/*credit to this video https://www.youtube.com/watch?v=LbSAG5aoKh0
VAR _columns = SELECTCOLUMNS(
    FILTER(
        INFO.VIEW.COLUMNS(),
        [Table] <> "Data Dictionary" && NOT([IsHidden])
    ),
    "Type", "Column",
    "Name", [Name],
    "Description", [Description],
    "Location", [Table],
    "Expression", [Expression]
)

VAR _measures = SELECTCOLUMNS(
    FILTER(
        INFO.VIEW.MEASURES(),
        [Table] <> "Data Dictionary" && NOT([IsHidden])
    ),
    "Type", "Measure",
    "Name", [Name],
    "Description", [Description],
    "Location", [Table],
    "Expression", [Expression]
)

VAR _tables = SELECTCOLUMNS(
    FILTER(
        INFO.VIEW.TABLES(),
        [Name] <> "Data Dictionary" && [Name] <> "Calculations" && NOT([IsHidden])
    ),
    "Type", "Table",
    "Name", [Name],
    "Description", [Description],
    "Location", BLANK(),
    "Expression", [Expression]
)

VAR _relationships = SELECTCOLUMNS(
    INFO.VIEW.RELATIONSHIPS(),
    "Type", "Relationship",
    "Name", [Relationship],
    "Description", BLANK(),
    "Location", [FromTable],
    "Expression", [Relationship]
)

RETURN
UNION(_columns, _measures, _tables, _relationships)


05: Dictionary = /* source https://github.com/powerbibro/powerbibro/tree/main

var _columns =              SELECTCOLUMNS
                            (
                            INFO.VIEW.COLUMNS()         ---- INFO VIEW
                            ,"Info Group", "Columns"    ---- INFO GROUP
                            ,"Info Group Order", 1      ---- INFO GROUP ORDER
                            ,"Alignment", [Alignment]
                            ,"AlternateOf", [AlternateOf]
                            ,"CalculationGroupPrecedence", BLANK()
                            ,"ColumnStorage", [ColumnStorage]
                            ,"CrossFilteringBehavior", BLANK()
                            ,"DataCategory", [DataCategory]
                            ,"DataType", [DataType]
                            ,"Description", [Description]
                            ,"DetailRowsDefinition", BLANK()
                            ,"DisplayFolder", IF([DisplayFolder] = BLANK(),"* No Folder",[DisplayFolder])
                            ,"Expression", [Expression]
                            ,"FormatString", [FormatString]
                            ---,"FormatStringDefinition", BLANK() -- UTILIZING FORMAT STRING
                            ,"FromCardinality", BLANK()
                            ,"FromColumn", BLANK()
                            ,"FromTable", BLANK()
                            ,"GroupingBehavior", [GroupingBehavior]
                            ,"ID", [ID]
                            ,"IsActive", BLANK()
                            ,"IsAvailableInMDX", [IsAvailableInMDX]
                            ,"IsHidden", [IsHidden]
                            ,"IsKey", [IsKey]
                            ,"IsNullable", [IsNullable]
                            ,"IsPrivate", BLANK()
                            ,"IsSimpleMeasure", BLANK()
                            ,"IsUnique", [IsUnique]
                            ,"KPIID", BLANK()
                            ,"LineageTag", [LineageTag]
                            ,"Model", BLANK()
                            ,"Name", [Name]
                            --,"Relationship", BLANK()
                            ,"RelyOnReferentialIntegrity", BLANK()
                            ,"SecurityFilteringBehavior", BLANK()
