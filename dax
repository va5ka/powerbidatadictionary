                            --,"State", BLANK()
                            ,"StorageMode", [StorageMode]
                            ,"SummarizeBy", BLANK()
                            ,"Table", [Name]    ---- POPULATE WITH TABLE NAME
                            ,"TableStorage", [TableStorage]
                            ,"ToCardinality", BLANK()
                            ,"ToColumn", BLANK()
                            ,"ToTable", BLANK() 
                            ,"Type", BLANK()
                            )

var _relationships_from =   SELECTCOLUMNS
                            (
                            INFO.VIEW.RELATIONSHIPS()       ---- INFO VIEW
                            ,"Info Group", "Relationships"  ---- INFO GROUP
                            ,"Info Group Order", 2          ---- INFO GROUP ORDER
                            ,"Alignment", BLANK()
                            ,"AlternateOf", BLANK()
                            ,"CalculationGroupPrecedence", BLANK()
                            ,"ColumnStorage", BLANK()
                            ,"CrossFilteringBehavior", [CrossFilteringBehavior]
                            ,"DataCategory", BLANK()
                            ,"DataType", [FromCardinality] & " to " & [ToCardinality] ---- concat cardinality
                            ,"Description", [CrossFilteringBehavior] -- create description
                            ,"DetailRowsDefinition", BLANK()
                            ,"DisplayFolder", "* Model Relationships" ---- MODEL RELATIONSHIPS
                            ,"Expression", BLANK()
                            ,"FormatString", BLANK()
                            --,"FormatStringDefinition", BLANK()-- UTILIZING FORMAT STRING
                            ,"FromCardinality", [FromCardinality]
                            ,"FromColumn", [FromColumn]
                            ,"FromTable", [FromTable]
                            ,"GroupingBehavior", BLANK()
                            ,"ID", [ID]
                            ,"IsActive", [IsActive]
                            ,"IsAvailableInMDX", BLANK()
                            ,"IsHidden", TRUE() ---- THESE ARE HIDDEN
                            ,"IsKey", BLANK()
                            ,"IsNullable", BLANK()
                            ,"IsPrivate", BLANK()
                            ,"IsSimpleMeasure", BLANK()
                            ,"IsUnique", BLANK()
                            ,"KPIID", BLANK()
                            ,"LineageTag", BLANK()
                            ,"Model", [Model]
                            --,"Name", [Name]
                            ,"Name", [Relationship] --- CONTAINS NAME
                            --,"Relationship", [Relationship] --- NOT NEEDED
                            ,"RelyOnReferentialIntegrity", [RelyOnReferentialIntegrity]
                            ,"SecurityFilteringBehavior", [SecurityFilteringBehavior]
                            ,"ShowAsVariationOnly", BLANK()
                            ,"SortByColumn", BLANK()
                            ,"SourceColumn", BLANK()
                            ,"SourceProviderType", BLANK()
                            --,"State", [State]
                            ,"StorageMode", BLANK()
                            ,"SummarizeBy", BLANK()
                            ,"Table", [FromTable]   ---- FROM TABLE
                            ,"TableStorage", BLANK()
                            ,"ToCardinality", [ToCardinality]
                            ,"ToColumn", [ToColumn]
                            ,"ToTable", [ToTable] 
                            ,"Type", BLANK()
                            )

var _relationships_to =     SELECTCOLUMNS
                            (
                            INFO.VIEW.RELATIONSHIPS()       ---- INFO VIEW
                            ,"Info Group", "Relationships"  ---- INFO GROUP
                            ,"Info Group Order", 2          ---- INFO GROUP ORDER
                            ,"Alignment", BLANK()
                            ,"AlternateOf", BLANK()
                            ,"CalculationGroupPrecedence", BLANK()
                            ,"ColumnStorage", BLANK()
                            ,"CrossFilteringBehavior", [CrossFilteringBehavior]
                            ,"DataCategory", BLANK()
                            ,"DataType", [FromCardinality] & " to " & [ToCardinality] ---- concat cardinality
                            ,"Description", [CrossFilteringBehavior] -- create description
                            ,"DetailRowsDefinition", BLANK()
                            ,"DisplayFolder", "* Model Relationships" ---- MODEL RELATIONSHIPS
                            ,"Expression", BLANK()
                            ,"FormatString", BLANK()
                            --,"FormatStringDefinition", BLANK() -- UTILIZING FORMAT STRING
                            ,"FromCardinality", [FromCardinality]
                            ,"FromColumn", [FromColumn]
                            ,"FromTable", [FromTable]
                            ,"GroupingBehavior", BLANK()
                            ,"ID", [ID]
                            ,"IsActive", [IsActive]
                            ,"IsAvailableInMDX", BLANK()
                            ,"IsHidden", TRUE() ---- THESE ARE HIDDEN
                            ,"IsKey", BLANK()
                            ,"IsNullable", BLANK()
                            ,"IsPrivate", BLANK()
                            ,"IsSimpleMeasure", BLANK()
                            ,"IsUnique", BLANK()
                            ,"KPIID", BLANK()
                            ,"LineageTag", BLANK()
                            ,"Model", [Model]
                            --,"Name", [Name]
                            ,"Name", [Relationship]             --- CONTAINS NAME
                            --,"Relationship", [Relationship]   --- NOT NEEDED
                            ,"RelyOnReferentialIntegrity", [RelyOnReferentialIntegrity]
                            ,"SecurityFilteringBehavior", [SecurityFilteringBehavior]
                            ,"ShowAsVariationOnly", BLANK()
                            ,"SortByColumn", BLANK()
                            ,"SourceColumn", BLANK()
                            ,"SourceProviderType", BLANK()
                            --,"State", [State]
                            ,"StorageMode", BLANK()
                            ,"SummarizeBy", BLANK()
                            ,"Table", [ToTable]                 ---- TO TABLE
                            ,"TableStorage", BLANK()
                            ,"ToCardinality", [ToCardinality]
                            ,"ToColumn", [ToColumn]
                            ,"ToTable", [ToTable] 
                            ,"Type", BLANK()
                            )


var _union      = UNION(_columns,_measures,_tables,_relationships_from,_relationships_to)   ---- UNION FOR MASTER DATASET

var _filter     = FILTER(_union,[DataCategory] <> "RowNumber")

RETURN

_filter

