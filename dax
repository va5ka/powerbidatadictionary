var _tables =               SELECTCOLUMNS
                            (INFO.VIEW.TABLES()             ---- INFO VIEW
                            ,"Info Group", "Tables"         ---- INFO GROUP
                            ,"Info Group Order", 0      ---- INFO GROUP ORDER
                            ,"Alignment", BLANK()
                            ,"AlternateOf", BLANK()
                            ,"CalculationGroupPrecedence", [CalculationGroupPrecedence]
                            ,"ColumnStorage", BLANK()
                            ,"CrossFilteringBehavior", BLANK()
                            ,"DataCategory", [DataCategory]
                            ,"DataType", [DataCategory] ---- DATA CATEGORY
                            ,"Description", [Description]
                            ,"DetailRowsDefinition", BLANK()
                            ,"DisplayFolder", "* Table Attributes" ---- TABLE
                            ,"Expression", [Expression]
                            ,"FormatString", [StorageMode] ---- STORAGE MODE
                            --,"FormatStringDefinition", BLANK() -- UTILIZING FORMAT STRING
                            ,"FromCardinality", BLANK()
                            ,"FromColumn", BLANK()
                            ,"FromTable", BLANK()
